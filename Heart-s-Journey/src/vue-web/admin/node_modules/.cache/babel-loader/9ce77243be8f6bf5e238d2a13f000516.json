th=\"50\" />\r\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='true'  \n\t\t\t\t\t\tprop=\"name\"\n\t\t\t\t\t\tlabel=\"心理测试名称\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.name}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='true'  \n\t\t\t\t\t\tprop=\"time\"\n\t\t\t\t\t\tlabel=\"测试时长(分钟)\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.time}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='true' prop=\"status\" label=\"心理测试状态\">\r\n\t\t\t\t\t\t<template slot-scope=\"scope\">\r\n\t\t\t\t\t\t\t<el-switch\r\n\t\t\t\t\t\t\t\tv-model=\"scope.row.status\"\r\n\t\t\t\t\t\t\t\t:width=\"40\"\r\n\t\t\t\t\t\t\t\tactive-text=\"启用\"\r\n\t\t\t\t\t\t\t\tinactive-text=\"禁用\"\r\n\t\t\t\t\t\t\t\t:active-value=\"1\"\r\n\t\t\t\t\t\t\t\t:inactive-value=\"0\"\r\n\t\t\t\t\t\t\t\tactive-color=\"#FCBB78\"\r\n\t\t\t\t\t\t\t\tinactive-color=\"#999\"\r\n\t\t\t\t\t\t\t\t@change=\"(e)=>statusChange(e,scope.row)\">\r\n\t\t\t\t\t\t\t</el-switch>\r\n\t\t\t\t\t\t</template>\r\n\t\t\t\t\t</el-table-column>\r\n\t\t\t\t\t<el-table-column width=\"300\" label=\"操作\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\r\n\t\t\t\t\t\t\t<el-button class=\"btn8\" v-if=\"isAuth('exampaper','组卷')\" @click=\"composeClick(scope.row)\" type=\"success\">\r\n\t\t\t\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\" :style='{\"margin\":\"0 2px\",\"fontSize\":\"14px\",\"color\":\"#fff\",\"display\":\"none\",\"height\":\"40px\"}'></span>\r\n\t\t\t\t\t\t\t\t组卷\r\n\t\t\t\t\t\t\t</el-button>\r\n\t\t\t\t\t\t\t<el-button class=\"view\" v-if=\" isAuth('exampaper','查看')\" type=\"success\" @click=\"addOrUpdateHandler(scope.row.id,'info')\">\r\n\t\t\t\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\" :style='{\"margin\":\"0 2px\",\"fontSize\":\"14px\",\"color\":\"#fff\",\"display\":\"none\",\"height\":\"40px\"}'></span>\r\n\t\t\t\t\t\t\t\t查看
load\" v-else-if=\"ruleForm.tupian\" label=\"图片\" prop=\"tupian\" >\r\n\t\t\t\t\t<img v-if=\"ruleForm.tupian.substring(0,4)=='http'\" class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" :src=\"ruleForm.tupian.split(',')[0]\" width=\"100\" height=\"100\">\r\n\t\t\t\t\t<img v-else class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in ruleForm.tupian.split(',')\" :src=\"$base.url+item\" width=\"100\" height=\"100\">\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"兑换积分\" prop=\"jifen\" >\r\n\t\t\t\t\t<el-input-number v-model=\"ruleForm.jifen\" placeholder=\"兑换积分\" :disabled=\"ro.jifen\"></el-input-number>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"兑换积分\" prop=\"jifen\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.jifen\" placeholder=\"兑换积分\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</template>\r\n\t\t\t<el-form-item class=\"textarea\" v-if=\"type!='info'\" label=\"徽章简介\" prop=\"huizhangjianjie\" >\r\n\t\t\t\t<el-input\r\n\t\t\t\t\tstyle=\"min-width: 200px; max-width: 600px;\"\r\n\t\t\t\t\ttype=\"textarea\"\r\n\t\t\t\t\t:rows=\"8\"\r\n\t\t\t\t\tplaceholder=\"徽章简介\"\r\n\t\t\t\t\tv-model=\"ruleForm.huizhangjianjie\" >\r\n\t\t\t\t</el-input>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item v-else-if=\"ruleForm.huizhangjianjie\" label=\"徽章简介\" prop=\"huizhangjianjie\" >\r\n\t\t\t\t<span class=\"text\">{{ruleForm.huizhangjianjie}}</span>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item class=\"btn\">\r\n\t\t\t\t<el-button class=\"btn3\"  v-if=\"type!='info'\" type=\"success\" @click=\"onSubmit\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t提交\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button class=\"btn4\" v-if=\"type!='info'\" type=\"success\" @click=\"back()\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t取消\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button c
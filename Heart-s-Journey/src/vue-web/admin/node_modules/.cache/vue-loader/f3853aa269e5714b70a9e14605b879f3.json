CA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"add-or-update.vue","sourceRoot":"src/views/modules/yonghu","sourcesContent":["<template>\r\n\t<div class=\"addEdit-block\">\r\n\t\t<el-form\r\n\t\t\tclass=\"add-update-preview\"\r\n\t\t\tref=\"ruleForm\"\r\n\t\t\t:model=\"ruleForm\"\r\n\t\t\t:rules=\"rules\"\r\n\t\t\tlabel-width=\"180px\"\r\n\t\t>\r\n\t\t\t<template >\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"用户账号\" prop=\"yonghuzhanghao\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.yonghuzhanghao\" placeholder=\"用户账号\" clearable  :readonly=\"ro.yonghuzhanghao\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"用户账号\" prop=\"yonghuzhanghao\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.yonghuzhanghao\" placeholder=\"用户账号\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"密码\" prop=\"mima\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.mima\" placeholder=\"密码\" clearable  :readonly=\"ro.mima\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"密码\" prop=\"mima\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.mima\" placeholder=\"密码\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"用户姓名\" prop=\"yonghuxingming\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.yonghuxingming\" placeholder=\"用户姓名\" clearable  :readonly=\"ro.yonghuxingming\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"用户姓名\" prop=\"yonghuxingming\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.yonghuxingming\" placeholder=\"用户姓名\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"upload\" v-if=\"type!='info' && !ro.touxiang\" label=\"头像\" prop=\"touxiang\" >\r\n\t\t\t\t\t<file-upload\r\n\t\t\t\t\t\ttip=\"点击上传头像\"\r\n\t\t\t\t\t\taction=\"file/upload\"\r\n\t\t\t\t\t\t:limit=\"3\"\r\n\t\t\t\t\t\t:multiple=\"true\"\r\n\t\t\t\t\t\t:fileUrls=\"ruleForm.touxiang?ruleForm.touxiang:''\"\r\n\t\t\t\t\t\t@change=\"touxiangUploadChange\"\r\n\t\t\t\t\t></file-upload>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"upload\" v-else-if=\"ruleForm.touxiang\" label=\"头像\" prop=\"touxiang\" >\r\n\t\t\t\t\t<img v-if=\"ruleForm.touxiang.substring(0,4)=='http'\" class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" :src=\"ruleForm.touxiang.split(',')[0]\" width=\"100\" height=\"100\">\r\n\t\t\t\t\t<img v-else class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in ruleForm.touxiang.split(',')\" :src=\"$base.url+item\" width=\"100\" height=\"100\">\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"性别\" prop=\"xingbie\" >\r\n\t\t\t\t\t<el-select :disabled=\"ro.xingbie\" v-model=\"ruleForm.xingbie\" placeholder=\"请选择性别\" >\r\n\t\t\t\t\t\t<el-option\r\n\t\t\t\t\t\t\tv-for=\"(item,index) in xingbieOptions\"\r\n\t\t\t\t\t\t\tv-bind:key=\"index\"\r\n\t\t\t\t\t\t\t:label=\"item\"\r\n\t\t\t\t\t\t\t:value=\"item\">\r\n\t\t\t\t\t\t</el-option>\r\n\t\t\t\t\t</el-select>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"性别\" prop=\"xingbie\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.xingbie\"\r\n\t\t\t\t\t\tplaceholder=\"性别\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"手机号码\" prop=\"shoujihaoma\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.shoujihaoma\" placeholder=\"手机号码\" clearable  :readonly=\"ro.shoujihaoma\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"手机号码\" prop=\"shoujihaoma\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.shoujihaoma\" placeholder=\"手机号码\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"积分\" prop=\"jifen\" >\r\n\t\t\t\t\t<el-input-number v-model=\"ruleForm.jifen\" placeholder=\"积分\" :disabled=\"ro.jifen\"></el-input-number>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"积分\" prop=\"jifen\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.jifen\" placeholder=\"积分\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</template>\r\n\t\t\t<el-form-item class=\"btn\">\r\n\t\t\t\t<el-button class=\"btn3\"  v-if=\"type!='info'\" type=\"success\" @click=\"onSubmit\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t提交\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button class=\"btn4\" v-if=\"type!='info'\" type=\"success\" @click=\"back()\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t取消\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button class=\"btn5\" v-if=\"type=='info'\" type=\"success\" @click=\"back()\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t返回\r\n\t\t\t\t</el-button>\r\n\t\t\t</el-form-item>\r\n\t\t</el-form>\r\n    \r\n\r\n\t</div>\r\n</template>\r\n<script>\r\n\timport { \r\n\t\tisNumber,\r\n\t\tisMobile,\r\n\t} from \"@/utils/validate\";\r\n\texport default {\r\n\t\tdata() {\r\n\t\t\tvar validateMobile = (rule, value, callback) => {\r\n\t\t\t\tif(!value){\r\n\t\t\t\t\tcallback();\r\n\t\t\t\t} else if (!isMobile(value)) {\r\n\t\t\t\t\tcallback(new Error(\"请输入正确的手机号码\"));\r\n\t\t\t\t} else {\r\n\t\t\t\t\tcallback();\r\n\t\t\t\t}\r\n\t\t\t};\r\n\t\t\tvar validateNumber = (rule, value, callback) => {\r\n\t\t\t\tif(!value){\r\n\t\t\t\t\tcallback();\r\n\t\t\t\t} else if (!isNumber(value)) {\r\n\t\t\t\t\tcallback(new Error(\"请输入数字\"));\r\n\t\t\t\t} else {\r\n\t\t\t\t\tcallback();\r\n\t\t\t\t}\r\n\t\t\t};\r\n\t\t\treturn {\r\n\t\t\t\tid: '',\r\n\t\t\t\ttype: '',\r\n\t\t\t\r\n\t\t\t\r\n\t\t\t\tro:{\r\n\t\t\t\t\tyonghuzhanghao : false,\r\n\t\t\t\t\tmima : false,\r\n\t\t\t\t\tyonghuxingming : false,\r\n\t\t\t\t\ttouxiang : false,\r\n\t\t\t\t\txingbie : false,\r\n\t\t\t\t\tshoujihaoma : false,\r\n\t\t\t\t\tjifen : false,\r\n\t\t\t\t},\r\n\t\t\t\r\n\t\t\t\truleForm: {\r\n\t\t\t\t\tyonghuzhanghao: '',\r\n\t\t\t\t\tmima: '',\r\n\t\t\t\t\tyonghuxingming: '',\r\n\t\t\t\t\ttouxiang: '',\r\n\t\t\t\t\txingbie: '',\r\n\t\t\t\t\tshoujihaoma: '',\r\n\t\t\t\t\tjifen: '0',\r\n\t\t\t\t},\r\n\t\t\r\n\t\t\t\txingbieOptions: [],\r\n\r\n\t\t\t\trules: {\r\n\t\t\t\t\tyonghuzhanghao: [\r\n\t\t\t\t\t\t{ required: true, message: '用户账号不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tmima: [\r\n\t\t\t\t\t\t{ required: true, message: '密码不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tyonghuxingming: [\r\n\t\t\t\t\t\t{ required: true, message: '用户姓名不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\ttouxiang: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\txingbie: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tshoujihaoma: [\r\n\t\t\t\t\t\t{ validator: validateMobile, trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tjifen: [\r\n\t\t\t\t\t\t{ validator: validateNumber, trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t},\r\n\t\t\t};\r\n\t\t},\r\n\t\tprops: [\"parent\"],\r\n\t\tcomputed: {\r\n\r\n\r\n\r\n\t\t},\r\n\t\tcomponents: {\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\t// 下载\r\n\t\t\tdownload(file){\r\n\t\t\t\twindow.open(`${file}`)\r\n\t\t\t},\r\n\t\t\t// 初始化\r\n\t\t\tinit(id,type) {\r\n\t\t\t\tif (id) {\r\n\t\t\t\t\tthis.id = id;\r\n\t\t\t\t\tthis.type = type;\r\n\t\t\t\t}\r\n\t\t\t\tif(this.type=='info'||this.type=='else'||this.type=='msg'){\r\n\t\t\t\t\tthis.info(id);\r\n\t\t\t\t}else if(this.type=='logistics'){\r\n\t\t\t\t\tthis.logistics=false;\r\n\t\t\t\t\tthis.info(id);\r\n\t\t\t\t}else if(this.type=='cross'){\r\n\t\t\t\t\tvar obj = this.$storage.getObj('crossObj');\r\n\t\t\t\t\tfor (var o in obj){\r\n\t\t\t\t\t\tif(o=='yonghuzhanghao'){\r\n\t\t
CA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"add.vue","sourceRoot":"src/pages/xuexiziyuan","sourcesContent":["<template>\r\n\t<div class=\"add-update-preview\">\r\n\t\t<el-form\r\n\t\t\tclass=\"add-update-form\"\r\n\t\t\tref=\"ruleForm\"\r\n\t\t\t:model=\"ruleForm\"\r\n\t\t\t:rules=\"rules\"\r\n\t\t\tlabel-width=\"100px\"\r\n\t\t\t>\r\n\t\t\t<el-form-item class=\"add-item\" label=\"资源名称\" prop=\"ziyuanmingcheng\">\r\n\t\t\t\t<el-input v-model=\"ruleForm.ziyuanmingcheng\" \r\n\t\t\t\t\tplaceholder=\"资源名称\" clearable :disabled=\" false  ||ro.ziyuanmingcheng\"></el-input>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item class=\"add-item\" label=\"资源图片\" v-if=\"type!='cross' || (type=='cross' && !ro.ziyuantupian)\" prop=\"ziyuantupian\">\r\n\t\t\t\t<file-upload\r\n\t\t\t\t\ttip=\"点击上传资源图片\"\r\n\t\t\t\t\taction=\"file/upload\"\r\n\t\t\t\t\t:limit=\"3\"\r\n\t\t\t\t\t:multiple=\"true\"\r\n\t\t\t\t\t:fileUrls=\"ruleForm.ziyuantupian?ruleForm.ziyuantupian:''\"\r\n\t\t\t\t\t@change=\"ziyuantupianUploadChange\"\r\n\t\t\t\t\t></file-upload>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item class=\"add-item\" v-else label=\"资源图片\" prop=\"ziyuantupian\">\r\n\t\t\t\t<img v-if=\"ruleForm.ziyuantupian.substring(0,4)=='http'\" class=\"upload-img\" v-bind:key=\"index\" :src=\"ruleForm.ziyuantupian.split(',')[0]\">\r\n\t\t\t\t<img v-else class=\"upload-img\" v-bind:key=\"index\" v-for=\"(item,index) in ruleForm.ziyuantupian.split(',')\" :src=\"baseUrl+item\">\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item class=\"add-item\"  label=\"资源类型\" prop=\"ziyuanleixing\">\r\n\t\t\t\t<el-select v-model=\"ruleForm.ziyuanleixing\" placeholder=\"请选择资源类型\" :disabled=\" false  ||ro.ziyuanleixing\" >\r\n\t\t\t\t\t<el-option\r\n\t\t\t\t\t\tv-for=\"(item,index) in ziyuanleixingOptions\"\r\n\t\t\t\t\t\t:key=\"index\"\r\n\t\t\t\t\t\t:label=\"item\"\r\n\t\t\t\t\t\t:value=\"item\">\r\n\t\t\t\t\t</el-option>\r\n\t\t\t\t</el-select>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item class=\"add-item\" label=\"学习积分\" prop=\"jifen\">\r\n\t\t\t\t<el-input-number v-model=\"ruleForm.jifen\" placeholder=\"学习积分\" :disabled=\" false ||ro.jifen\"></el-input-number>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item class=\"add-item\" label=\"学习视频\" v-if=\"type!='cross' || (type=='cross' && !ro.xuexishipin)\" prop=\"xuexishipin\">\r\n\t\t\t\t<file-upload\r\n\t\t\t\t\ttip=\"点击上传学习视频\"\r\n\t\t\t\t\taction=\"file/upload\"\r\n\t\t\t\t\t:limit=\"1\"\r\n\t\t\t\t\t:type=\"2\"\r\n\t\t\t\t\t:multiple=\"true\"\r\n\t\t\t\t\t:fileUrls=\"ruleForm.xuexishipin?ruleForm.xuexishipin:''\"\r\n\t\t\t\t\t@change=\"xuexishipinUploadChange\"\r\n\t\t\t\t\t></file-upload>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item class=\"add-item\" v-else label=\"学习视频\" prop=\"xuexishipin\">\r\n\t\t\t\t<div class=\"viewBtn\" v-if=\"ruleForm.xuexishipin\" @click=\"download(baseUrl + ruleForm.xuexishipin)\" type=\"success\">预览</div>\r\n\t\t\t\t<div class=\"unviewBtn\" v-else disabled>暂无</div>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item class=\"add-item\" label=\"资源文件\" prop=\"ziyuanwenjian\">\r\n\t\t\t\t<file-upload\r\n\t\t\t\t\ttip=\"点击上传资源文件\"\r\n\t\t\t\t\taction=\"file/upload\"\r\n\t\t\t\t\t:limit=\"1\"\r\n\t\t\t\t\t:type=\"3\"\r\n\t\t\t\t\t:multiple=\"true\"\r\n\t\t\t\t\t:fileUrls=\"ruleForm.ziyuanwenjian?ruleForm.ziyuanwenjian:''\"\r\n\t\t\t\t\t@change=\"ziyuanwenjianUploadChange\"\r\n\t\t\t\t\t></file-upload>\r\n\t\t\t</el-form-item>  \r\n\t\t\t<el-form-item class=\"add-item\" label=\"发布时间\" prop=\"fabushijian\">\r\n\t\t\t\t<el-date-picker\r\n\t\t\t\t\t:disabled=\" false  ||ro.fabushijian\"\r\n\t\t\t\t\tvalue-format=\"yyyy-MM-dd HH:mm:ss\"\r\n\t\t\t\t\tv-model=\"ruleForm.fabushijian\" \r\n\t\t\t\t\ttype=\"datetime\"\r\n\t\t\t\t\tplaceholder=\"发布时间\">\r\n\t\t\t\t</el-date-picker>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item class=\"add-item\" label=\"资源内容\" prop=\"ziyuanneirong\">\r\n\t\t\t\t<editor \r\n\t\t\t\t\tv-model=\"ruleForm.ziyuanneirong\" \r\n\t\t\t\t\tclass=\"editor\" \r\n\t\t\t\t\taction=\"file/upload\">\r\n\t\t\t\t</editor>\r\n\t\t\t</el-form-item>\r\n\r\n\t\t\t<el-form-item class=\"add-btn-item\">\r\n\t\t\t\t<el-button class=\"submitBtn\"  type=\"primary\" @click=\"onSubmit\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-kaitongfuwu\"></span>\r\n\t\t\t\t\t<span class=\"text\">提交</span>\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button class=\"closeBtn\" @click=\"back()\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-shanchu1\"></span>\r\n\t\t\t\t\t<span class=\"text\">取消</span>\r\n\t\t\t\t</el-button>\r\n\t\t\t</el-form-item>\r\n\t\t</el-form>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\texport default {\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tid: '',\r\n\t\t\t\tbaseUrl: '',\r\n\t\t\t\tro:{\r\n\t\t\t\t\tziyuanmingcheng : false,\r\n\t\t\t\t\tziyuantupian : false,\r\n\t\t\t\t\tziyuanleixing : false,\r\n\t\t\t\t\tjifen : false,\r\n\t\t\t\t\txuexishipin : false,\r\n\t\t\t\t\tziyuanwenjian : false,\r\n\t\t\t\t\tziyuanneirong : false,\r\n\t\t\t\t\tfabushijian : false,\r\n\t\t\t\t\tclicktime : false,\r\n\t\t\t\t\tstoreupnum : false,\r\n\t\t\t\t},\r\n\t\t\t\ttype: '',\r\n\t\t\t\tuserTableName: localStorage.getItem('UserTableName'),\r\n\t\t\t\truleForm: {\r\n\t\t\t\t\tziyuanmingcheng: '',\r\n\t\t\t\t\tziyuantupian: '',\r\n\t\t\t\t\tziyuanleixing: '',\r\n\t\t\t\t\tjifen: '',\r\n\t\t\t\t\txuexishipin: '',\r\n\t\t\t\t\tziyuanwenjian: '',\r\n\t\t\t\t\tziyuanneirong: '',\r\n\t\t\t\t\tfabushijian: '',\r\n\t\t\t\t\tclicktime: '',\r\n\t\t\t\t\tstoreupnum: '',\r\n\t\t\t\t},\r\n\t\t\t\tziyuanleixingOptions: [],\r\n\r\n\r\n\t\t\t\trules: {\r\n\t\t\t\t\tziyuanmingcheng: [\r\n\t\t\t\t\t\t{ required: true, message: '资源名称不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tziyuantupian: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tziyuanleixing: [\r\n\t\t\t\t\t\t{ required: true, message: '资源类型不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tjifen: [\r\n\t\t\t\t\t\t{ required: true, message: '学习积分不能为空', trigger: 'blur' },\r\n\t\t\t\t\t\t{ validator: this.$validate.isNumber, trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\txuexishipin: [\r\n\t\t\t\t\t\t{ required: true, message: '学习视频不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tziyuanwenjian: [\r\n\t\t\t\t\t\t{ required: true, message: '资源文件不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tziyuanneirong: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tfabushijian: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tclicktime: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tstoreupnum: [\r\n\t\t\t\t\t\t{ validator: this.$validate.isIntNumer, trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t},\r\n\t\t\t\tcenterType: false,\r\n\t\t\t};\r\n\t\t},\r\n\t\tcomputed: {\r\n\r\n\r\n\r\n\t\t},\r\n\t\tcomponents: {\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t\tif(this.$route.query.centerType){\r\n\t\t\t\tthis.centerType = true\r\n\t\t\t}\r\n\t\t\t//this.bg();\r\n\t\t\tlet type = this.$route.query.type ? this.$route.query.type : '';\r\n\t\t\tthis.init(type);\r\n\t\t\tthis.baseUrl = this.$config.baseUrl;\r\n\t\t\tthis.ruleForm.fabushijian = this.getCurDateTime()\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\tgetMakeZero(s) {\r\n\t\t\t\treturn s < 10 ? '0' + s : s;\r\n\t\t\t},\r\n\t\t\t// 下载\r\n\t\t\tdownload(file){\r\n\t\t\t\twindow.open(`${file}`)\r\n\t\t\t},\r\n\t\t\t// 初始化\r\n\t\t\tinit(type) {\r\n\t\t\t\tthis.type = type;\r\n\t\t\t\tif(type=='cross'){\r\n\t\t\t\t\tvar obj = JSON.parse(localStorage.getItem('crossObj'));\r\n\t\t\t\t\tfor (var o in obj){\r\n\t\t\t\t\t\tif(o=='ziyuanmingcheng'){\r\n\t\t\t\t\t\t\tthis.ruleForm.ziyuanmingcheng = obj[o];\r\n\t\t\t\t\t\t\tthis.ro.ziyuanmingcheng = true;\r\n\t\t\t\t\t\t\tcontinue;\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tif(o=='ziyuantupian'){\r\n\t\t\t\t\t\t\tthis.ruleForm.ziyuantupian = obj[o].split(\",\")[0];\r\n\t\t\t\t\t\t\tthis.ro.ziyuantupian = true;\r\n\t\t\t\t\t\t\tcontinue;\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tif(o=='ziyuanleixing'){\r\n\t\t\t\t\t\t\tthis.ruleForm.ziyuanleixing = obj[o];\r\n\t\t\t\t